<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kavara.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_stat_onesignal_default"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_stat_onesignal_default"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SharedPref"></activity>
        <activity android:name=".AddQuotes" />
        <activity android:name=".QuotesActivity" />
        <activity android:name=".CategoryActivity" />
        <activity android:name=".FirstActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-8024531215248997~8430977680" />
        <meta-data
            android:name="com.onesignal.NotificationAccentColor.DEFAULT"
            android:value="e52165" />
    </application>

</manifest>